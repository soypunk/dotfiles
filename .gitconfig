[user]
	name = Shawn Medero
	email = soypunk@gmail.com
[core]
	excludesfile = ~/.gitignore
	editor = bbedit --wait --resume
[github]
	user = soypunk
[diff]
	tool = "bbdiff"
[difftool]
	prompt = false
[difftool "bbdiff"]
	cmd = /usr/local/bin/bbdiff --wait --resume "$LOCAL" "$REMOTE"
[alias]
	st = status
	ci = commit
	co = checkout
